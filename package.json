{
  "name": "browserscreen-chrome",
  "version": "1.0.0",
  "description": "Chrome Extension that resizes a video on a website to fit the current browser screen. <br /> This extension only works with one video on a website! <br /> Fullscreen is not fully working and will not be worked on in the future. <br /> Missing features due to no personal needs.",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config webpack/webpack.dev.js --watch",
    "build": "webpack --config webpack/webpack.prod.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "publish": "web-ext sign -s dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myin142/browserscreen-chrome.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/myin142/browserscreen-chrome/issues"
  },
  "homepage": "https://github.com/myin142/browserscreen-chrome#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.25.0",
    "ts-loader": "^9.4.2",
    "typescript": "^3.5.2",
    "web-ext": "^7.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "plyr": "^3.7.2"
  }
}